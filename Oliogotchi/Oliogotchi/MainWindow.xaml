<Window x:Class="Oliogotchi.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Oliogotchi"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <StackPanel VerticalAlignment="Bottom" Margin="0,0,0,50" Width="150" Height="100">
            <Button x:Name="btnJatka" Content="Jatka" Margin="0,0,0,5"/>
            <Button x:Name="btnUusi" Content="Uusi peli" Margin="0,0,0,5"/>
            <Button x:Name="btnLopeta" Content="Lopeta" Margin="0,0,0,5"/>
            <Button x:Name="btnAsetukset" Content="Asetukset" Margin="0,0,0,5"/>
        </StackPanel>
    </Grid>
</Window>
